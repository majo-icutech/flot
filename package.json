{
  "name": "flot",
  "version": "4.2.3",
  "main": "dist/es5/jquery.flot.js",
  "scripts": {
    "test": "node node_modules/karma/bin/karma start --single-run --no-auto-watch --concurrency=1 --stopOnEsLintError",
    "karma": "node node_modules/karma/bin/karma start --no-auto-watch",
