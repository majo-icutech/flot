    "karma-coveralls": "^2.1.0",
    "karma-edge-launcher": "^0.4.2",
    "karma-eslint": "^2.2.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "karma-safari-launcher": "^1.0.0",
